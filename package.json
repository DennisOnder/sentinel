{
  "name": "sentinel",
  "version": "1.0.0",
  "description": "Password Manager with TypeScript, Express, ArangoDB and React",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon .",
    "client": "cd src/client && npm run dev",
    "dev": "concurrently \"npm start\" \"npm run client\"",
    "prep": "npm install && cd src/client && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DennisOnder/sentinel.git"
  },
  "keywords": [
    "TypeScript",
    "React",
    "ArangoDB",
    "Express"
  ],
  "author": "Dennis Onder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DennisOnder/sentinel/issues"
  },
  "homepage": "https://github.com/DennisOnder/sentinel#readme",
  "dependencies": {
    "arangojs": "^5.8.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/crypto-js": "^3.1.43",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^11.11.3",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.9.0",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "nodemon": "^1.18.10",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  }
}
